<?xml version="1.0" ?>

<world name="demo_world" gravity="0.0 0.0 0.0">

	<robot name="PANDA">
		<model dir="./resources" path="panda_arm.urdf" name="panda_arm" />
	</robot>

	<static_object name="Crokiboard">
		<origin xyz="0.6 0.0 0.4" rpy="0 0 0" />
	    <collision>
	        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
	        <geometry>
	            <cylinder length="0.1" radius="0.5" />
	        </geometry>
	    </collision>
	    <visual>
	        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
	        <geometry>
	            <cylinder length="0.02" radius="0.3" />
	        </geometry>
	        <material name="material_gray">
		        <color rgba="0.6 0.6 0.6 1.0" />
	        </material>
	    </visual>
	</static_object>


	<light name="light1" type="directional">
		<position xyz="2.0 -2.0 2.0" />
		<lookat xyz="0.0 0.0 0.0" />
	</light>

	<light name="light2" type="directional">
		<position xyz="2.0 2.0 2.0" />
		<lookat xyz="0.0 0.0 0.0" />
	</light>

	<camera name="camera_fixed">
		<position xyz="1.5 1.0 1.0" />
		<vertical xyz="0.0 0.0 1.0" />
		<lookat xyz="0.0 0.0 0.4" />
	</camera>

</world>
